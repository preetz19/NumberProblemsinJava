package org.test;
import java.util.*;

public class Number {
public static void main (String args[])
{
	Scanner sc=new Scanner(System.in);
	int a=sc.nextInt();
	System.out.println("Enter the value"+" "+a);
	if(a>=0)
	{
		System.out.println("The given value is positive");
		
	}
	else
	{
		System.out.println("The given value is negative");
	}
}
}
