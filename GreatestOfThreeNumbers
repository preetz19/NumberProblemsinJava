package org.test;
import java.util.*;
public class Number {
public static void main(String args[])
{
	Scanner sc=new Scanner(System.in);
	int a=sc.nextInt();
	int b=sc.nextInt();
	int c=sc.nextInt();
	if(a>b && a>c)
	{
		System.out.println("A is greatest number");
	}
	if( b>a && b>c)
	{
		System.out.println("B is greatest number");
	}
	if ( c>a && c>b)
	{
		System.out.println("C is greatest number");
	}
}
}
